<tr id=<%= "estado_civil_#{estado_civil.id}" %>>
   <td><%= estado_civil.nombre %></td>
    <td><%= link_to estado_civil, class: "btn btn-info btn-sm", remote: true do %>
            <span class="glyphicon glyphicon-eye-open"></span>
            <% end %>
    </td>
    <td>
        <%= link_to edit_estado_civil_path(estado_civil), class: "btn btn-warning btn-sm", remote: true do %>
            <span class="glyphicon glyphicon-edit"></span>
        <% end %>
    </td>
    <td>
        <%= link_to estado_civil, method: :delete, data: { confirm: 'Estas seguro?' }, remote: true, class: "btn btn-danger btn-sm" do %>
            <span class="glyphicon glyphicon-remove"></span>
        <% end %>
    </td>
</tr>   