<tr id=<%= "cliente_#{cliente.id}" %>>
   <td><%= cliente.nombre %></td>
        <td><%= cliente.apellido %></td>
        <td><%= cliente.telefono1 %></td>
        <td><%= cliente.mail %></td>
        <td><%= cliente.docIdentidad %></td>
        <td><%= cliente.tMovil %></td>
        <td><%= link_to cliente, class: "btn btn-info btn-sm", remote: true do %>
          <span class="glyphicon glyphicon-eye-open"></span>
        <% end %>
        </td>
      
        <td>
          <%= link_to edit_cliente_path(cliente), class: "btn btn-warning btn-sm", remote: true do %>
            <span class="glyphicon glyphicon-edit"></span>
          <% end %>
        </td>
        
        <td>
          <%= link_to cliente, method: :delete, data: { confirm: 'Estas seguro?' }, remote: true, class: "btn btn-danger btn-sm" do %>
            <span class="glyphicon glyphicon-remove"></span>
          <% end %>
        </td>
        
        <td>
          <%= link_to cliente_path(cliente, :format => 'pdf'), class: "btn btn-default" do %>
            <span class="glyphicon glyphicon-cloud-download"></span>
          <% end %>
        </td>
</tr>   