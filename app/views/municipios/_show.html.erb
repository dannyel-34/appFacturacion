<div class='municipio'>
<p id="notice"><%= notice %></p>

<p>
  <strong>Nombre departamento:</strong>
  <%= @municipio.nombre_departamento %>
</p>

<p>
  <strong>Nombre municipio:</strong>
  <%= @municipio.nombre_municipio %>
</p>

<p>
  <strong>Nombre centro poblado:</strong>
  <%= @municipio.nombre_centro_poblado %>
</p>

<p>
  <strong>Nombre:</strong>
  <%= @municipio.nombre %>
</p>

<center>
<%= link_to edit_municipio_path(@municipio), class: "btn btn-warning" do %>
<span class="glyphicon glyphicon-edit"></span>
<% end %> |
<%= link_to municipios_path, class: "btn btn-success" do %>
<span class="glyphicon glyphicon-share-alt"></span>
<% end %>
</center>
</div>