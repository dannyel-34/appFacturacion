<tr id=<%= "municipio_#{municipio.id}" %>>
   <td><%= municipio.nombre %></td>
         <td><%= link_to municipio, class: "btn btn-info btn-sm", remote: true do %>
          <span class="glyphicon glyphicon-eye-open"></span>
        <% end %>
        </td>
      
        <td>
          <%= link_to edit_municipio_path(municipio), class: "btn btn-warning btn-sm", remote: true do %>
            <span class="glyphicon glyphicon-edit"></span>
          <% end %>
        </td>
        
        <td>
          <%= link_to municipio, method: :delete, data: { confirm: 'Estas seguro?' }, remote: true, class: "btn btn-danger btn-sm" do %>
            <span class="glyphicon glyphicon-remove"></span>
          <% end %>
        </td>
</tr>